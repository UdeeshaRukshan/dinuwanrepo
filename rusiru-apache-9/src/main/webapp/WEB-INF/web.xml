<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>com.Admin.AdminServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>AdminInsert</servlet-name>
	<servlet-class>com.Admin.AdminInsertServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>AdminInsert</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>UpdateAdmin</servlet-name>
	<servlet-class>com.Admin.AdminUpdateServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>UpdateAdmin</servlet-name>
		<url-pattern>/update</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>DeleteAdmin</servlet-name>
		<servlet-class>com.Admin.deleteAdminServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DeleteAdmin</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>
	
	
	
	
	
    
	
	

</web-app>



